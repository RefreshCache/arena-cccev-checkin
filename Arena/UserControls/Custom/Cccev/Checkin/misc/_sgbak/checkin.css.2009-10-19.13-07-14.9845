html
{
	overflow: hidden;
}

body
{
	margin: 0px;
	padding: 0px;
	overflow: hidden;
	font-family: "Cooper Black", "Cooper Black BT", "Arial Rounded MT Bold";
}
e
a, input, div, span, li, td, p, span.footerText
{
	font-family: "Cooper Black", "Cooper Black BT", "Arial Rounded MT Bold";
}

.container
{
	width: 1024px;
	height: 768px;
	background-image: url(/Arena/UserControls/Custom/Cccev/Checkin/images/checkin_bg.png);
	background-repeat: no-repeat;
}

.view
{
	width: 1024px;
	height: 768px;
	vertical-align: top;
	text-align: center;
}

.initView
{
	width: 1024px;
	height: 768px;
	vertical-align: bottom;
}

div.heading
{
	border: none;
	text-align: center;
	padding: 20px;
}

div.footer
{
	background-image: url(/Arena/UserControls/Custom/Cccev/Checkin/images/footer_bg.png);
	background-repeat: no-repeat;
	width: 1024px;
	height: 118px;
	position: absolute;
	bottom: 0;
	left: 0;
}

.invis
{
	visibility: hidden;
}

.footerText
{
	color: #FFFFFF;
	font-size: 32pt;
	display: block;
	margin: 40px 0 0 20px;
    padding-bottom: 25px;
}

.footerCaption
{
	color: #FFFFFF;
	font-size: 24pt;
	display: block;
	margin: 40px 0 0 20px;
    padding-bottom: 25px;
}

.footerError
{
	text-decoration: underline;
	color: #FFFFFF;
	font-size: 32pt;
	display: block;
	margin: 40px 0 0 20px;
    padding-bottom: 25px;
}

.footerLeft
{
	float: left;
	width: 500px;
	height: 118px;
	text-align: left;
}

.footerRight
{
	float: right;
	width: 500px;
	text-align: right;
}

.footerWide
{
	width: 1024px;
	height: 118px;
	text-align: center;
}

.checkinText
{
	font-size: 36pt;
	color: #000000;
}

