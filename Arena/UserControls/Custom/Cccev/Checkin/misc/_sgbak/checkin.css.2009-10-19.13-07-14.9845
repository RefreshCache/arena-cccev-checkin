.ajaxProgress
{
	margin-left: auto ;
	margin-right: 0 ;
}

.ajaxLargeProgress
{
	background-color: #000;
	
	padding: 0px;
	color: #000;
	font-family: Arial;
	font-size: 30pt;
	font-weight: normal;
	opacity:.50;
	filter: alpha(opacity=50); 
	-moz-opacity: 0.5;
	text-align: center;
	position: absolute;
	top: 0;
	left: 0;
	background-image: url(/Arena/UserControls/Custom/Cccev/Checkin/images/big_roller.gif);
	/* background-attachment:fixed; */
	background-position: 50%;
	background-repeat: no-repeat;
	width: 1024px;
	height: 768px;
}