{
	font-size: 48pt;
	color: #000000;
	border: none;
}

.confirmText
{
	font-size: 36pt;
	color: #000000;
}

.classroomText
{
	font-size: 36pt;
	color: #009900;
	
	filter: Shadow(Color=#000000, Direction=135, Strength=5);
}

